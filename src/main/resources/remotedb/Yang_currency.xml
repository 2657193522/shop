<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.randioo.market_server.dao_remote.CurrencyDAO">
	<!-- select -->
	<select id="getType" resultType="String">
		select 
			currency_name
		from 
			yang_currency
		where 
		currency_id=#{currency_id}
	</select>
	<!-- select -->
	<!-- select -->
	<select id="getTypeId" resultType="String">
		select 
			detail_url
		from 
			yang_currency
		where 
		currency_id=#{currency_id}
	</select>
	<!-- select -->
</mapper>